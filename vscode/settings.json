{
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    "editor.formatOnType": true,
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[vue][javascript][typescript][json][jsonc][html][javascriptreact][css][typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "breadcrumbs.enabled": true,

  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.autoClosingBrackets": "never",
  "editor.autoSurround": "never",
  "editor.cursorStyle": "line",
  "editor.fontSize": 11,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.insertSpaces": true,
  "editor.lineNumbers": "on",
  "editor.minimap.enabled": false,
  "editor.suggestSelection": "first",
  "editor.tabSize": 4,
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "on",

  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "files.watcherExclude": {
    "**/.git/**": true,
    "**/node_modules/**": true,
    "**/tensorboard_env/**": true,
    "**/*.gz": true,
    "**/logs": true,
    "*.db": true,
    "*.parquet": true,
    "*.pickle": true
  },

  "html.autoClosingTags": false,
  "javascript.autoClosingTags": false,
  "typescript.autoClosingTags": false,

  "jupyter.askForKernelRestart": false,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },

  "redhat.telemetry.enabled": false,

  "terminal.integrated.fontFamily": "Monaco, 'Courier New', monospace",
  "terminal.integrated.fontSize": 11,
  "terminal.integrated.inheritEnv": false,

  "vim.insertModeKeyBindings": [{ "before": ["q", "q"], "after": ["<Esc>"] }],
  "vim.useSystemClipboard": true,
  "vim.whichwrap": "h,l,<,>,[,]",

  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",

  "window.title": "${rootName}${separator}${activeEditorShort}",
  "workbench.colorCustomizations": {
    "editor.background": "#ffeac1",
    "editor.foreground": "#1d5097",
    "editor.selectionBackground": "#fcab16",
    "terminal.ansiBrightWhite": "#926400"
  },
  "workbench.colorTheme": "Solarized Light",
  "workbench.editor.showTabs": "single",
  "workbench.editorAssociations": { "*.ipynb": "jupyter-notebook" },
  "workbench.startupEditor": "none",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": false,
    "markdown": false
  },
  "editor.autoClosingQuotes": "never",
  "playwright.reuseBrowser": false,
  "python.defaultInterpreterPath": "/usr/local/Caskroom/miniconda/base/bin/python",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "git.mergeEditor": true,
  "prettier.trailingComma": "all",
  "liveServer.settings.donotVerifyTags": true,
  "html.autoCreateQuotes": false,
  "svelte.enable-ts-plugin": true,
  "workbench.iconTheme": "vscode-icons",
  "window.zoomLevel": 1,
  "files.autoSaveWhenNoErrors": true,
  "terminal.integrated.env.linux": {},
  "python.envFile": "",
  "git.openRepositoryInParentFolders": "never",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "github.copilot.editor.enableAutoCompletions": true,
  "[svelte]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "notebook.output.wordWrap": true
}
