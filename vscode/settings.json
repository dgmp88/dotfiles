{
  "[python]": {
    "editor.codeActionsOnSave": { "source.organizeImports": true }
  },
  "[vue][javascript][typescript][json][jsonc][html][javascriptreact][css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "breadcrumbs.enabled": true,

  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.autoClosingBrackets": "never",
  "editor.autoSurround": "never",
  "editor.cursorStyle": "line",
  "editor.fontSize": 11,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.insertSpaces": true,
  "editor.lineNumbers": "on",
  "editor.minimap.enabled": false,
  "editor.suggestSelection": "first",
  "editor.tabSize": 4,
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",

  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "files.watcherExclude": {
    "**/.git/**": true,
    "**/node_modules/**": true,
    "**/tensorboard_env/**": true,
    "**/*.gz": true,
    "**/logs": true,
    "*.db": true,
    "*.parquet": true,
    "*.pickle": true
  },

  "html.autoClosingTags": false,
  "jupyter.askForKernelRestart": false,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },

  "python.formatting.blackPath": "/usr/local/Caskroom/miniconda/base/bin/black",
  "python.formatting.provider": "black",
  "python.sortImports.path": "/usr/local/Caskroom/miniconda/base/bin/isort",

  "redhat.telemetry.enabled": false,

  "terminal.integrated.fontFamily": "Monaco, 'Courier New', monospace",
  "terminal.integrated.fontSize": 11,
  "terminal.integrated.inheritEnv": false,

  "vim.insertModeKeyBindings": [{ "before": ["q", "q"], "after": ["<Esc>"] }],
  "vim.useSystemClipboard": true,
  "vim.whichwrap": "h,l,<,>,[,]",

  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",

  "window.title": "${rootName}${separator}${activeEditorShort}",
  "window.zoomLevel": 1,

  "workbench.activityBar.visible": true,
  "workbench.colorCustomizations": {
    "editor.background": "#ffeac1",
    "editor.foreground": "#1d5097",
    "editor.selectionBackground": "#fcab16",
    "terminal.ansiBrightWhite": "#926400"
  },
  "workbench.colorTheme": "Solarized Light",
  "workbench.editor.showTabs": false,
  "workbench.editorAssociations": { "*.ipynb": "jupyter-notebook" },
  "workbench.startupEditor": "none"
}
